<StreetTemplates>
    <StreetTemplate>
        <name>SouthNorth</name>
        <imagePath>./images/streets/NS.png</imagePath>
        <LaneTemplates>
            <LaneTemplate>
                <from>N</from>
                <to>S</to>
            </LaneTemplate>
            <LaneTemplate>
                <from>S</from>
                <to>N</to>
            </LaneTemplate>
        </LaneTemplates>

    </StreetTemplate>
    <StreetTemplate>
        <name>WestEast</name>
           <imagePath>./images/streets/WE.png</imagePath>
        <LaneTemplates>
            <LaneTemplate>
                <from>W</from>
                <to>E</to>
            </LaneTemplate>
            <LaneTemplate>
                <from>E</from>
                <to>W</to>
            </LaneTemplate>
        </LaneTemplates>
    </StreetTemplate>

    <StreetTemplate>
        <name>Bridge</name>
        <imagePath>./images/streets/Bridge.png</imagePath>
        <LaneTemplates>
            <LaneTemplate>
                <from>W</from>
                <to>E</to>
            </LaneTemplate>
            <LaneTemplate>
                <from>E</from>
                <to>W</to>
            </LaneTemplate>
            <LaneTemplate>
                <from>N</from>
                <to>S</to>
            </LaneTemplate>
            <LaneTemplate>
                <from>S</from>
                <to>N</to>
            </LaneTemplate>
        </LaneTemplates>
    </StreetTemplate>

    <StreetTemplate>
        <name>Intersection</name>
        <imagePath>./images/streets/intersection.png</imagePath>
        <LaneTemplates>


            <LaneTemplate>
                <from>E</from>
                <to>S</to>
            </LaneTemplate>
            <LaneTemplate>
                <from>E</from>
                <to>N</to>

            </LaneTemplate>
            <LaneTemplate>
                <from>E</from>
                <to>W</to>
            </LaneTemplate>
            <LaneTemplate>
                <from>S</from>
                <to>E</to>
            </LaneTemplate>
            <LaneTemplate>
                <from>S</from>
                <to>W</to>
            </LaneTemplate>
            <LaneTemplate>
                <from>S</from>
                <to>N</to>
            </LaneTemplate>
            <LaneTemplate>
                <from>W</from>
                <to>S</to>
            </LaneTemplate>
            <LaneTemplate>
                <from>W</from>
                <to>N</to>
            </LaneTemplate>
            <LaneTemplate>
                <from>W</from>
                <to>E</to>
            </LaneTemplate>
            <LaneTemplate>
                <from>N</from>
                <to>E</to>
            </LaneTemplate>

            <LaneTemplate>
                <from>N</from>
                <to>W</to>
            </LaneTemplate>
            <LaneTemplate>
                <from>N</from>
                <to>S</to>
            </LaneTemplate>


        </LaneTemplates>
    </StreetTemplate>

    <StreetTemplate>
      <name>BendSW</name>
        <imagePath>./images/streets/BendWS.png</imagePath>
        <LaneTemplates>
            <LaneTemplate>
                <from>W</from>
                <to>S</to>
            </LaneTemplate>
             <LaneTemplate>
                <from>S</from>
                <to>W</to>
            </LaneTemplate>

        </LaneTemplates>
    </StreetTemplate>

     <StreetTemplate>
      <name>BendWN</name>
         <imagePath>./images/streets/BendWN.png</imagePath>
        <LaneTemplates>
            <LaneTemplate>
                <from>W</from>
                <to>N</to>
            </LaneTemplate>
             <LaneTemplate>
                <from>N</from>
                <to>W</to>
            </LaneTemplate>

        </LaneTemplates>
    </StreetTemplate>

     <StreetTemplate>
      <name>BendES</name>
         <imagePath>./images/streets/BendSE.png</imagePath>
        <LaneTemplates>
            <LaneTemplate>
