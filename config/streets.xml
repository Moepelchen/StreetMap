            </LaneTemplate>

        </LaneTemplates>
    </StreetTemplate>

     <StreetTemplate>
      <name>BendEN</name>
         <imagePath>./images/streets/BendNE.png</imagePath>
        <LaneTemplates>
            <LaneTemplate>
                <from>E</from>
                <to>N</to>
            </LaneTemplate>
             <LaneTemplate>
